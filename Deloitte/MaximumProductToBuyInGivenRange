import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int a[] = {3,1,2,1,1};
	    int k = 5;
	    Arrays.sort(a);
	    int b = 0;
	    int count = 0;
	    for(int i = 0 ; i< a.length;i++){
	        b += a[i];
	        if(b <=5){
	            count++;
	            
	        }
	    }
	    System.out.print(count);
	}
}
